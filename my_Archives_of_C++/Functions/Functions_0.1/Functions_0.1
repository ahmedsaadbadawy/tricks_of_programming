#include<bits/stdc++.h>
using namespace std;
void fun(int &a,int &b)
{
    int tmp =a;
    a=b;
    b=tmp;
}

int main() {


    int x, y,z;
    cin >>x>>y>>z;
    int arr[x][x];
    for(int i=0;i<x;i++)
    {
        for(int j=0;j<x;j++)
    {
        cin>>arr[i][j];
    }
    }

    for(int j=0;j<x;j++)
    {
        fun(arr[y-1][j],arr[z-1][j]);
    }
    for(int j=0;j<x;j++)
    {

        fun(arr[j][y-1],arr[j][z-1]);
    }
    for(int i=0;i<x;i++)
    {
        for(int j=0;j<x;j++)
    {
       cout<<arr[i][j]<<" ";
    }
    cout<<endl;
    }

    return 0;
}

